<?xml version='1.0' encoding='utf-8'?>
<widget id="org.t2.tricaremedicine" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MHS</name>
    <description>
        Military Health System
    </description>
    <author email="dave.coleman@tee2.org" href="http://tee2.org">
        T2
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <preference name="Orientation" value="portrait" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <icon src="res/icon/icon-72-2x.png" />
    <platform name="android">
        <allow-intent href="market:*" />
        <splash density="port-hdpi" src="res/screen/android/screen-hdpi-portrait.png" />
        <splash density="port-ldpi" src="res/screen/android/screen-hdpi-portrait.png" />
        <splash density="port-mdpi" src="res/screen/android/screen-hdpi-portrait.png" />
        <splash density="port-xhdpi" src="res/screen/android/screen-hdpi-portrait.png" />
    </platform>
    <platform name="ios">
        <splash src="res/screen/ios/Default@2x~universal~anyany.png" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="29" src="res/icon/app-icon-29.png" width="29" />
        <icon height="40" src="res/icon/app-icon-40.png" width="40" />
        <icon height="50" src="res/icon/app-icon-50.png" width="50" />
        <icon height="57" src="res/icon/app-icon-57.png" width="57" />
        <icon height="58" src="res/icon/app-icon-58.png" width="58" />
        <icon height="72" src="res/icon/app-icon-72.png" width="72" />
        <icon height="76" src="res/icon/app-icon-76.png" width="76" />
        <icon height="80" src="res/icon/app-icon-80.png" width="80" />
        <icon height="87" src="res/icon/app-icon-87.png" width="87" />
        <icon height="100" src="res/icon/app-icon-100.png" width="100" />
        <icon height="114" src="res/icon/app-icon-114.png" width="114" />
        <icon height="120" src="res/icon/app-icon-120.png" width="120" />
        <icon height="144" src="res/icon/app-icon-144.png" width="144" />
        <icon height="152" src="res/icon/app-icon-152.png" width="152" />
        <icon height="167" src="res/icon/app-icon-167.png" width="167" />
        <icon height="180" src="res/icon/app-icon-180.png" width="180" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-open-native-settings" spec="^1.4.1" />
    <plugin name="cordova-plugin-device" spec="^1.1.6" />
    <plugin name="cordova-plugin-geolocation" spec="^2.4.3">
        <variable name="GEOLOCATION_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="^2.2.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-sqlite-storage" spec="^2.0.4" />
</widget>
